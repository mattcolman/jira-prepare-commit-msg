{
  "name": "husky6-test",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "husky": "^6.0.0"
  },
  "scripts": {
    "postinstall": "husky install && husky add .husky/prepare-commit-msg \"node ../../../bin/index.js --verbose \"\\$1\"\""
  },
  "jira-prepare-commit-msg": {
    "isConventionalCommit": true,
    "messagePattern": "[$J]. $M",
    "allowEmptyCommitMessage": true
  }
}
