{
  "name": "husky4-test",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "husky": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "node ../../../bin/index.js --verbose"
    }
  },
  "jira-prepare-commit-msg": {
    "isConventionalCommit": true,
    "messagePattern": "[$J]. $M",
    "allowEmptyCommitMessage": true
  }
}
